    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-xml-parser": "^5.2.5",
    "jsdom": "^26.1.0",
    "openai": "^5.10.2",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
